{
  "name": "tuto_navGrid",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "browserify -g reactify ./public/javascripts/App.jsx --minified -o ./public/javascripts/bundle.js -t",
    "watch": "watchify -v -g  reactify -p [ browserify-livereload --host 127.0.0.1 --port 1338 ] ./public/javascripts/App.jsx -o ./public/javascripts/bundle.js -t",
    "clean": "rm ./public/javascripts/bundle.js"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "ejs": "~2.3.3",
    "express": "~4.13.1",
    "material-ui": "^0.16.6",
    "morgan": "~1.6.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-tap-event-plugin": "^2.0.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "browserify-livereload": "^1.0.8",
    "reactify": "^1.1.1",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  }
}
